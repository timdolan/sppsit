      <header id="header">
        <%= render '/refinery/header' -%>
      </header>
      
     <div class="content_outer_wrap">
      <div class="content_wrap">
	<div id="home_body">
		  <%= yield %>
	</div>
	
	<div id="base_links"><%= render '/refinery/base_links' %></div>
